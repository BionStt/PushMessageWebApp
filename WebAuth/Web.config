  <elmah>
    <security allowRemoteAccess="true" />
    <errorLog type="Elmah.XmlFileErrorLog, Elmah" logPath="~/App_Data/ElmahXML_Logs" />
    <errorPost targetUrl="http://localhost:4143/elmah" sourceId="simple" secret="haha" />
